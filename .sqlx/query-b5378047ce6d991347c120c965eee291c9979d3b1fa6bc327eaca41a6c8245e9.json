  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                r.id,\n                r.created_date_time,\n                r.modification_date_time,\n                r.resource_name,\n                r.ven_id,\n                r.attributes,\n                r.targets as \"targets:Vec<Target>\"\n            FROM resource r\n                JOIN ven v on r.ven_id = v.id\n            WHERE r.id = $1\n              AND ($2::text IS NULL OR v.client_id = $2)\n            ",
