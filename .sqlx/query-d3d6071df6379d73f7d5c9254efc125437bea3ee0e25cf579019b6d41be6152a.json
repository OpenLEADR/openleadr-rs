{
  "db_name": "PostgreSQL",
  "query": "\n            UPDATE report r\n            SET modification_date_time = now(),\n                program_id = $6,\n                event_id = $7,\n                client_name = $8,\n                report_name = $9,\n                payload_descriptors = $10,\n                resources = $11\n            FROM program p\n            WHERE r.id = $1\n              AND (p.id = r.program_id)\n              AND (\n                  ($2 AND r.ven_id = ANY($3))\n                  OR\n                  ($4 AND ($5::text[] IS NULL OR p.business_id = ANY ($5)))\n                  )\n            RETURNING r.*\n            ",
