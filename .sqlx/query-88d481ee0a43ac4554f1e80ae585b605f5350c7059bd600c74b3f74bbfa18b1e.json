  "db_name": "PostgreSQL",
  "query": "\n            SELECT e.id,\n                   e.created_date_time,\n                   e.modification_date_time,\n                   e.program_id,\n                   e.event_name,\n                   e.priority,\n                   e.targets as \"targets:Vec<Target>\",\n                   e.report_descriptors,\n                   e.payload_descriptors,\n                   e.interval_period,\n                   e.intervals\n            FROM event e\n                     JOIN program p ON e.program_id = p.id\n            WHERE e.id = $1\n            ",
